LIBDIR = ./rust/target/release
STATLIB = $(LIBDIR)/librpolars.a
PKG_LIBS = -L$(LIBDIR) -lrpolars

all: C_clean

$(SHLIB): $(STATLIB)

$(STATLIB):
	#cargo build --lib --release --manifest-path=./rust/Cargo.toml

C_clean:
	rm -Rf $(SHLIB) $(OBJECTS) #$(STATLIB)

clean:
	rm -Rf $(SHLIB) $(STATLIB) $(OBJECTS) rust/target
